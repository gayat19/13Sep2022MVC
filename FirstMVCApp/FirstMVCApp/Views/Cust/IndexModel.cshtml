@model  IEnumerable<FirstMVCApp.Models.Customer>

<table border="1">
    @foreach (var item in Model)
    {
    <tr>
        <td>
            @item.Id
        </td>
        <td>
            @item.Name
        </td>
        <td>
            @item.Age
        </td>
        <td>
            @item.EMail
        </td>
        <td>
            @item.Phone
        </td>
    </tr>
    }
</table>